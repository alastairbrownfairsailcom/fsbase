<aura:component access="global">
	<aura:attribute name="process" type="fsbase.FSProcessDTO" required="true"/>
    
    <!-- events fired -->
	<aura:registerEvent name="processSelected" type="fsbase:FSProcessSelectedEvent"/>

    <!-- body -->
    <p>Process Name: {!v.process.displayName}<ui:button label="Select" press="{!c.selectProcess}" /></p>
    
</aura:component>